{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["app/*"],
      "@components/*": ["app/components/*"],
      "@screens/*": ["app/screens/*"],
      "@services/*": ["app/services/*"],
      "@utils/*": ["app/utils/*"],
      "@assets/*": ["app/assets/*"],
      "@interfaces/*": ["app/interfaces/*"],
      "@navigate/*": ["app/navigate/*"]
    }
  }
}
